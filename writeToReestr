lic = ''
FIO = ''
firm = ''
c = wmi.WMI(computer=Comp, user='dsdsd'), password='dfdf')
filenam = r'cmd.exe /c reg add "HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Installer\UserData\S-1-5-18\Products\7AB5E7046046FB044ACD63458B5F481C\InstallProperties" /f /v ProductID /t REG_SZ /d ' + lic
            process_startup = c.Win32_ProcessStartup.new()
            process_id, result = c.Win32_Process.Create(CommandLine=filenam,   ProcessStartupInformation=process_startup)
            filenam1 = r'cmd.exe /c reg add "HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Installer\UserData\S-1-5-18\Products\7AB5E7046046FB044ACD63458B5F481C\InstallProperties" /f /v RegOwner /t REG_SZ /d ' + '"' + FIO + '"'
            process_id, result = c.Win32_Process.Create(CommandLine=filenam1,   ProcessStartupInformation=process_startup)
            firm = r'ООО Рога и копыта'
            filenam1 = r'cmd.exe /c reg add "HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Installer\UserData\S-1-5-18\Products\7AB5E7046046FB044ACD63458B5F481C\InstallProperties" /f /v RegCompany /t REG_SZ /d ' + '"' + firm + '"'
            process_id, result = c.Win32_Process.Create(CommandLine=filenam1,   ProcessStartupInformation=process_startup)

